{
  "kind": "collectionType",
  "collectionName": "topics",
  "info": {
    "singularName": "topic",
    "pluralName": "topics",
    "displayName": "Hashpower Blog Topic"
  },
  "options": {
    "draftAndPublish": false
  },
  "attributes": {
    "name": {
      "type": "string",
      "unique": true,
      "required": true
    },
    "slug": {
      "type": "uid",
      "targetField": "name"
    },
    "blogs": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::blog.blog",
      "mappedBy": "topics"
    }
  }
}
