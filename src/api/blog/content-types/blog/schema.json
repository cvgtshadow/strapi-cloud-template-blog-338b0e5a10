{
  "kind": "collectionType",
  "collectionName": "blogs",
  "info": {
    "singularName": "blog",
    "pluralName": "blogs",
    "displayName": "Hashpower Blog Article",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "title": {
      "type": "string",
      "required": true
    },
    "description": {
      "type": "text",
      "required": true
    },
    "slug": {
      "type": "uid",
      "required": true,
      "targetField": "title"
    },
    "topics": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::topic.topic",
      "mappedBy": "topics"
    },
    "authors": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::author.author",
      "inversedBy": "blogs"
    },
    "blocks": {
      "type": "dynamiczone",
      "components": ["shared.rich-text", "shared.media"]
    },
    "tableOfContents": {
      "type": "json"
    },
    "publishedDate": {
      "type": "datetime"
    }
  }
}
